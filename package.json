{
  "name": "koa-knex-realworld-example",
  "version": "1.0.0",
  "description": "conduit on koa with knex",
  "main": "src/bin/server",
  "repository": "git@github.com:gothinkster/koa-knex-realworld-example.git",
  "author": "Dmitrii Solovev <dimonnwc3@gmail.com>",
  "license": "ISC",
  "scripts": {
    "start": "node src/bin/server",
    "dev": "nodemon src/bin/server",
    "db:load": "knex seed:run",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:currentVersion": "knex migrate:currentVersion",
    "lint": "eslint \"**/*.js\"",
    "format": "prettier --write \"**/*.js\"",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "jest": {
    "bail": true,
    "testEnvironment": "node",
    "coverageReporters": [
      "text"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "standard",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    }
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "dependencies": {
    "bcrypt": "3.0.6",
    "config": "3.1.0",
    "date-fns": "1.30.1",
    "dotenv": "8.0.0",
    "humps": "2.0.1",
    "join-js": "1.0.2",
    "jsonwebtoken": "8.5.1",
    "kcors": "2.2.2",
    "knex": "0.16.5",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-helmet": "4.1.0",
    "koa-jwt": "3.5.1",
    "koa-logger": "3.2.0",
    "koa-response-time": "2.1.0",
    "koa-router": "7.4.0",
    "koa-x-request-id": "2.0.0",
    "lodash": "4.17.11",
    "mississippi": "4.0.0",
    "p-event": "4.1.0",
    "pg": "7.10.0",
    "pino": "5.12.3",
    "pino-colada": "1.4.4",
    "qs": "6.7.0",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "slug": "1.1.0",
    "sqlite3": "4.0.6",
    "stoppable": "1.1.0",
    "uuid": "3.3.2",
    "validator": "10.11.0",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "faker": "4.1.0",
    "husky": "2.2.0",
    "jest": "24.8.0",
    "lint-staged": "8.1.6",
    "nodemon": "1.19.0",
    "prettier": "1.17.0"
  }
}
